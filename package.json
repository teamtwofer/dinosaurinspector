{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "Project management software",
  "main": "index.js",
  "scripts": {
    "start": "psql $DATABASE_URL -f init.sql; npm run build:server && npm run build && cp -r public serverBuild",
    "build": "webpack --config webpack.production.config.js",
    "build:server": "tsc --project tsconfig.server.json",
    "postinstall": "npm run start",
    "server": "NODE_ENV=production npx pm2 start ./serverBuild/server -i max --attach",
    "server:dev": "NODE_ENV=development ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:ci": "npm run lint && npm run lint:prettier:list",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --single-quote --trailing-comma=es5 --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:prettier:list": "prettier --single-quote --trailing-comma=es5 -l \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix",
    "test": "jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Team Twofer",
  "private": true,
  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0"
  },
  "dependencies": {
    "@nestjs/common": "^3.0.1",
    "@nestjs/core": "^3.0.1",
    "@nestjs/microservices": "^3.0.1",
    "@nestjs/testing": "^3.0.0",
    "@nestjs/websockets": "^3.0.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.4",
    "@types/classnames": "2.2.2",
    "@types/compression": "0.0.33",
    "@types/core-decorators": "^0.10.32",
    "@types/express": "^4.0.36",
    "@types/express-enforces-ssl": "^1.1.0",
    "@types/helmet": "0.0.36",
    "@types/jest": "20.0.7",
    "@types/jsonwebtoken": "7.2.3",
    "@types/morgan": "^1.7.32",
    "@types/node": "8.0.24",
    "@types/postmark": "^1.3.0",
    "@types/react": "15.6.1",
    "@types/react-dom": "15.5.3",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-router-dom": "^4.0.7",
    "@types/ua-parser-js": "^0.7.31",
    "@types/webpack": "3.0.9",
    "@types/webpack-dev-middleware": "^1.9.2",
    "@types/webpack-dev-server": "2.4.1",
    "@types/webpack-hot-middleware": "^2.15.0",
    "@urbandoor/storage": "0.0.2",
    "autoprefixer": "^7.1.3",
    "awesome-typescript-loader": "3.2.3",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "class-validator": "^0.7.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "core-decorators": "^0.19.0",
    "css-loader": "0.28.5",
    "cssnano": "^3.10.0",
    "express": "4.15.4",
    "express-enforces-ssl": "^1.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "formstate": "0.23.0",
    "helmet": "3.8.1",
    "html-webpack-plugin": "2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "jsonwebtoken": "7.4.3",
    "material-ui": "^1.0.0-beta.8",
    "mobx": "3.2.2",
    "mobx-react": "^4.2.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "pg": "6.4.2",
    "pg-connection-string": "^0.1.3",
    "pm2": "2.6.1",
    "postcss": "6.0.9",
    "postcss-loader": "^2.0.6",
    "postmark": "^1.4.1",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.2",
    "react-router-dom": "4.1.2",
    "reflect-metadata": "^0.1.10",
    "resolve-url-loader": "^2.1.0",
    "rxjs": "^5.0.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "20.0.10",
    "ts-node": "3.3.0",
    "tslint": "5.6.0",
    "tslint-react": "3.2.0",
    "tsutils": "2.8.0",
    "typedi": "^0.5.2",
    "typeorm": "0.1.0-alpha.34",
    "typescript": "2.4.2",
    "ua-parser-js": "^0.7.14",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "serverBuild"
    ]
  },
  "devDependencies": {
    "uglify-js": "^2.8.29"
  }
}
