{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "Project management software",
  "main": "index.js",
  "scripts": {
    "start": "psql $DATABASE_URL -f init.sql; npm run build:server && npm run build && cp -r public serverBuild",
    "build": "webpack --config webpack.production.config.js",
    "build:server": "tsc --project tsconfig.server.json",
    "postinstall": "npm run start",
    "server": "NODE_ENV=production npx pm2 start ./serverBuild/server -i max --attach",
    "server:dev": "NODE_ENV=development ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:ci": "npm run lint && npm run lint:prettier:list",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --single-quote --trailing-comma=es5 --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:prettier:list": "prettier --single-quote --trailing-comma=es5 -l \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix",
    "test": "jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Team Twofer",
  "private": true,
  "engines": {
    "node": "^8.1.0",
    "npm": "^5.3.0"
  },
  "dependencies": {
    "@nestjs/common": "^3.0.1",
    "@nestjs/core": "^3.0.1",
    "@nestjs/microservices": "^3.0.1",
    "@nestjs/testing": "^3.0.0",
    "@nestjs/websockets": "^3.0.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.4",
    "@types/classnames": "^2.2.0",
    "@types/compression": "0.0.33",
    "@types/core-decorators": "^0.10.32",
    "@types/express": "^4.0.36",
    "@types/express-enforces-ssl": "^1.1.0",
    "@types/helmet": "0.0.35",
    "@types/jest": "^19.2.4",
    "@types/jsonwebtoken": "^7.2.2",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.37",
    "@types/postmark": "^1.3.0",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-router-dom": "^4.0.7",
    "@types/ua-parser-js": "^0.7.31",
    "@types/webpack": "^2.2.16",
    "@types/webpack-dev-middleware": "^1.9.2",
    "@types/webpack-dev-server": "^2.4.0",
    "@types/webpack-hot-middleware": "^2.15.0",
    "@urbandoor/storage": "0.0.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "class-validator": "^0.7.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "core-decorators": "^0.19.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "express-enforces-ssl": "^1.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "formstate": "^0.21.1",
    "helmet": "^3.6.1",
    "html-webpack-plugin": "^2.29.0",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.4.1",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "pg": "^6.4.1",
    "pg-connection-string": "^0.1.3",
    "pm2": "^2.5.0",
    "postcss": "^6.0.6",
    "postmark": "^1.4.1",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.1",
    "reflect-metadata": "^0.1.10",
    "resolve-url-loader": "^2.1.0",
    "rxjs": "^5.0.3",
    "sass-loader": "^6.0.6",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "tsutils": "^2.7.1",
    "typedi": "^0.5.2",
    "typeorm": "0.1.0-alpha.19",
    "typescript": "^2.4.1",
    "ua-parser-js": "^0.7.14",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "serverBuild"
    ]
  },
}
