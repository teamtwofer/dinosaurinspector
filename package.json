{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "A booking interface",
  "main": "index.js",
  "scripts": {
    "server": "tsc --project tsconfig.server.json && node ./server production",
    "server:dev": "ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --single-quote --trailing-comma es5 --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix"
  },
  "author": "Urbandoor",
  "private": true,
  "engines": {
    "node": "^8.1.0",
    "npm": "^5.0.1"
  },
  "dependencies": {
    "@nestjs/common": "^3.0.0",
    "@nestjs/core": "^3.0.0",
    "@nestjs/microservices": "^3.0.0",
    "@nestjs/testing": "^3.0.0",
    "@nestjs/websockets": "^3.0.0",
    "@types/react-hot-loader": "^3.0.1",
    "express": "^4.15.3",
    "http-proxy": "^1.16.2",
    "morgan": "^1.8.2",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.31",
    "@types/react": "^15.0.28",
    "@types/react-dom": "^15.5.0",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-dev-server": "^2.4.0",
    "@types/webpack-hot-middleware": "^2.15.0",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.25.0",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.2",
    "prettier": "^1.4.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  }
}
