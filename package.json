{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "Project management software",
  "main": "index.js",
  "scripts": {
    "start": "psql $DATABASE_URL -f init.sql; npm run build:server && npm run build && cp -r public serverBuild",
    "build": "webpack --config webpack.production.config.js",
    "build:server": "tsc --project tsconfig.server.json",
    "postinstall": "npm run start",
    "server": "NODE_ENV=production npx pm2 start ./serverBuild/server -i max --attach",
    "server:dev": "NODE_ENV=development ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:ci": "npm run lint && npm run lint:prettier:list",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --single-quote --trailing-comma=es5 --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:prettier:list": "prettier --single-quote --trailing-comma=es5 -l \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix",
    "test": "jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Team Twofer",
  "private": true,
  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0"
  },
  "dependencies": {
    "@nestjs/common": "^4.1.1",
    "@nestjs/core": "^4.1.3",
    "@nestjs/microservices": "^4.1.0",
    "@nestjs/testing": "^4.1.0",
    "@nestjs/websockets": "^4.1.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.7",
    "@types/classnames": "^2.2.3",
    "@types/compression": "0.0.34",
    "@types/core-decorators": "^0.10.32",
    "@types/express": "^4.0.39",
    "@types/express-enforces-ssl": "^1.1.1",
    "@types/helmet": "0.0.37",
    "@types/jest": "^21.1.5",
    "@types/jsonwebtoken": "7.2.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.47",
    "@types/postmark": "^1.3.1",
    "@types/react": "^16.0.19",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-router-dom": "^4.2.0",
    "@types/ua-parser-js": "^0.7.32",
    "@types/webpack": "^3.0.14",
    "@types/webpack-dev-middleware": "^1.9.3",
    "@types/webpack-dev-server": "^2.9.1",
    "@types/webpack-hot-middleware": "^2.16.0",
    "@urbandoor/storage": "0.0.2",
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "3.2.3",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "class-validator": "^0.7.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "core-decorators": "^0.20.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "express": "^4.16.2",
    "express-enforces-ssl": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "formstate": "^0.24.0",
    "helmet": "^3.9.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^21.2.1",
    "jsonwebtoken": "^8.1.0",
    "material-ui": "^1.0.0-beta.19",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "pg": "^7.3.0",
    "pg-connection-string": "^2.0.0",
    "pm2": "2.6.1",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.8",
    "postmark": "^1.4.1",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reflect-metadata": "^0.1.10",
    "resolve-url-loader": "^2.2.0",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.4",
    "ts-node": "3.3.0",
    "tslint": "^5.8.0",
    "tslint-react": "3.2.0",
    "tsutils": "^2.12.1",
    "typedi": "^0.5.2",
    "typeorm": "^0.1.1",
    "typescript": "^2.5.3",
    "ua-parser-js": "^0.7.17",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "serverBuild"
    ]
  },
  "devDependencies": {
    "uglify-js": "^3.1.6"
  }
}
