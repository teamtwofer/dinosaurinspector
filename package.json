{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "Project management software",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:server && npm run build",
    "build": "webpack --config webpack.production.config.js",
    "build:server": "tsc --project tsconfig.server.json",
    "server": "npm run start && NODE_ENV=production node ./serverBuild/server",
    "server:dev": "NODE_ENV=development ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --single-quote --no-bracket-spacing=false --trailing-comma=es5 --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix",
    "test": "jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Team Twofer",
  "private": true,
  "engines": {
    "node": "^8.1.0",
    "npm": "^5.0.1"
  },
  "dependencies": {
    "@nestjs/common": "^3.0.0",
    "@nestjs/core": "^3.0.1",
    "@nestjs/microservices": "^3.0.1",
    "@nestjs/testing": "^3.0.0",
    "@nestjs/websockets": "^3.0.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/compression": "0.0.33",
    "@types/core-decorators": "^0.10.32",
    "@types/express": "^4.0.35",
    "@types/jest": "^19.2.4",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.31",
    "@types/react": "^15.0.28",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router-dom": "^4.0.4",
    "@types/webpack": "^2.2.15",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-dev-server": "^2.4.0",
    "@types/webpack-hot-middleware": "^2.15.0",
    "@urbandoor/storage": "0.0.2",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "core-decorators": "^0.19.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "formstate": "^0.21.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.4.1",
    "mobx": "^3.1.15",
    "mobx-react": "^4.2.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "pg": "^6.2.4",
    "postcss": "^6.0.2",
    "prettier": "^1.5.2",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "reflect-metadata": "^0.1.10",
    "resolve-url-loader": "^2.0.3",
    "rxjs": "^5.4.1",
    "sass-loader": "^6.0.6",
    "sendgrid": "^5.1.1",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.0.6",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "tsutils": "^2.5.1",
    "typedi": "^0.5.2",
    "typeorm": "0.1.0-alpha.19",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "unmockedModulePathPatterns": ["react"],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2"
  }
}
