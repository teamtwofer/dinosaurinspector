{
  "name": "dinosaurinspector",
  "version": "0.0.1",
  "description": "Project management software",
  "main": "index.js",
  "scripts": {
    "start": "psql $DATABASE_URL -f init.sql; npm run build:server && npm run build && cp -r public serverBuild",
    "build": "webpack --config webpack.production.config.js",
    "build:server": "tsc --project tsconfig.server.json",
    "postinstall": "npm run start",
    "server": "NODE_ENV=production npx pm2 start ./serverBuild/server -i max --attach",
    "server:dev": "NODE_ENV=development ts-node --project tsconfig.server.json ./server/index.ts",
    "server:watch": "nodemon",
    "lint": "tslint -t stylish \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:ci": "npm run lint && npm run lint:prettier:list",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier --write \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:prettier:list": "prettier -l \"{client,lang,server,types,__{tests,mocks}__}/**/*.t{sx,s}\"",
    "lint:fmt": "npm run lint:prettier && npm run lint:fix",
    "test": "jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Team Twofer",
  "private": true,
  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.2",
    "@nestjs/core": "^4.5.2",
    "@nestjs/microservices": "^4.5.2",
    "@nestjs/testing": "^4.5.2",
    "@nestjs/websockets": "^4.5.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/classnames": "^2.2.3",
    "@types/compression": "0.0.35",
    "@types/core-decorators": "^0.10.32",
    "@types/express": "^4.11.0",
    "@types/express-enforces-ssl": "^1.1.1",
    "@types/helmet": "0.0.37",
    "@types/jest": "^21.1.9",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.5.2",
    "@types/postmark": "^1.3.1",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-router-dom": "^4.2.3",
    "@types/ua-parser-js": "^0.7.32",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-middleware": "^1.12.0",
    "@types/webpack-dev-server": "^2.9.2",
    "@types/webpack-hot-middleware": "^2.16.2",
    "@urbandoor/storage": "0.0.2",
    "ajv": "^5.5.2",
    "ajv-keywords": "^2.1.1",
    "autoprefixer": "^7.2.3",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bucketing": "^0.1.1",
    "class-validator": "^0.7.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "core-decorators": "^0.20.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "express": "^4.16.2",
    "express-enforces-ssl": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "formstate": "^0.24.0",
    "helmet": "^3.9.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^22.0.4",
    "jsonwebtoken": "^8.1.0",
    "material-ui": "^1.0.0-beta.25",
    "mobx": "^3.4.1",
    "mobx-failable": "0.0.4",
    "mobx-react": "^4.3.5",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "pg": "^7.4.0",
    "pg-connection-string": "^2.0.0",
    "pm2": "^2.9.1",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "postmark": "^1.5.0",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reflect-metadata": "^0.1.10",
    "resolve-url-loader": "^2.2.1",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-react": "3.2.0",
    "tsutils": "^2.14.0",
    "typedi": "^0.6.0",
    "typeorm": "^0.1.9",
    "typescript": "^2.6.2",
    "ua-parser-js": "^0.7.17",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "serverBuild"
    ]
  },
  "devDependencies": {
    "uglify-js": "^3.3.2"
  }
}
